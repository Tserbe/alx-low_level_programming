#include "main.h"
/**
  * _strncat  - main entry
  * @dest: destination
  * @src: source
  * @n: to be explained later
  * Return: returns dest
  */
char *_strncat(char *dest, char *src, int n)
{
int a, b;
a = 0;
b = 0;

while (dest[a] != '\0')
{
a++;
}
while (b < n && src[b] != '\0')
{
dest[b] = src[b];
a++;
b++;
}
dest[a] = '\0';
return (dest);
}
